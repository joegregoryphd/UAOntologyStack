@dc:creator "SIE Disruption Lab"
@dc:contributor "Joe Gregory"
@dc:description "A vocabulary to capture data management patterns"
@dc:hasVersion "0.1"
vocabulary <http://siedepartment.com/SIE_Domain/SIE_DataManagement#> as SIE_DataManagement {
	
	////////////////////////////////////////////////////////////////////////////////////
	
	/////// Vocabulary Imports ///////
	
	
	// Import standard vocabularies	
	
    extends <http://purl.org/dc/elements/1.1/> as dc
    extends <http://www.w3.org/2000/01/rdf-schema#> as rdfs
    extends <http://www.w3.org/2001/XMLSchema#> as xsd
    
    // Import relevant vocabularies
    
    extends <http://siedepartment.com/SIE_Foundation/SIE_Foundation#> as foundation
	
	extends <http://siedepartment.com/SIE_Core/SIE_Information#> as info
	extends <http://siedepartment.com/SIE_Core/SIE_Agent#> as agent
	//extends <http://siedepartment.com/SIE_Core/SIE_Event#> as event
	//extends <http://siedepartment.com/SIE_Core/SIE_Measurement#> as meas
	//extends <http://siedepartment.com/SIE_Core/SIE_Provenance#> as prov
	
	//extends <http://siedepartment.com/SIE_Domain/SIE_SystemArchitecture#> as sa
	//extends <http://siedepartment.com/SIE_Domain/SIE_Mission#> as mission
	//extends <http://siedepartment.com/SIE_Domain/SIE_Requirements#> as req
	//extends <http://siedepartment.com/SIE_Domain/SIE_Test#> as test
	extends <http://siedepartment.com/SIE_Domain/SIE_Software#> as sw
		
	
	///////////////////////////////////////////////////////////////////////////////////
	
	/////// Vocabulary Definitions ///////
	
	///// Aspects
	
			
	
	///// Concepts
	
	@rdfs:label "Digital Location"
	@dc:description "A [=DigitalLocation=] is a..."
	concept DigitalLocation :> foundation:ImmaterialEntity
	
	@rdfs:label "Repository"
	@dc:description "A [=Repository=] is a..."
	concept Repository :> DigitalLocation
	
	
		
	///// Relations
	
	@rdfs:label "Has Access"
	@dc:description "[=HasAccess=] is a "
	relation entity HasAccess [
		from agent:Agent
		to DigitalLocation
		@rdfs:label "has access"
		forward hasAccess
		@rdfs:label "is accessible by"
		reverse isAccessibleBy
		asymmetric
		irreflexive
	]
	
	@rdfs:label "Has Digital Location"
	@dc:description "[=HasDigitalLocation=] is a "
	relation entity HasDigitalLocation [
		from info:InformationArtifact
		to DigitalLocation
		@rdfs:label "has digital location"
		forward hasDigitalLocation
		@rdfs:label "is digital location of"
		reverse isDigitalLocationOf
		asymmetric
		irreflexive
	]
	
	@rdfs:label "Supports Repository"
	@dc:description "[=SupportsRepository=] is a "
	relation entity SupportsRepository [
		from sw:Software
		to DigitalLocation
		@rdfs:label "supports repository"
		forward supportsRepository
		@rdfs:label "is supported by software"
		reverse isSupportedBySoftware
		asymmetric
		irreflexive
	]
	
	
	
	///// Scalar Properties
	
	@rdfs:label "has URL"
	@dc:description "[=hasURL=] ..."
	scalar property hasURL [
		domain DigitalLocation
		range xsd:string
	]
		
	
	
	///// Rules
		
	
}